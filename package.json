{
  "name": "ng2d3",
  "version": "1.3.0",
  "description": "Angular2 + D3 Charting Library",
  "main": "release/ng2d3.js",
  "typings": "release/ng2d3.d.ts",
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "start": "webpack-dev-server --progress --watch --inline",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "build": "webpack --progress --profile",
    "build:release": "NODE_ENV=production npm run build",
    "release": "npm-run-all -s clean:* release:css release:ts -p release:build:* -s release:minify",
    "release:ts": "ngc -p tsconfig.release.json",
    "release:build:umd": "rollup -c -f umd -o release/ng2d3.umd.js",
    "release:build:cjs": "rollup -c -f cjs -o release/ng2d3.cjs.js",
    "release:build:es": "rollup -c -f es -o release/ng2d3.es.js",
    "release:css": "node-sass -o release/ src/ng2d3.scss",
    "release:minify": "uglifyjs release/ng2d3.js --source-map release/ng2d3.min.js.map --source-map-url release/ng2d3.js.map --compress --mangle --screw-ie8 --output release/ng2d3.min.js",
    "deploy:demo": "npm run build:release && node ./tasks/deploy-gh-page.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ng2d3.git"
  },
  "keywords": [
    "angular2",
    "angularjs",
    "charts",
    "charting",
    "angular",
    "d3"
  ],
  "authors": [
    "Austin McDaniel",
    "Marjan Georgiev",
    "Olivier Combe"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/ng2d3/issues"
  },
  "homepage": "https://github.com/swimlane/ng2d3#readme",
  "peerDependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "core-js": "2.4.1",
    "d3": "4.2.2",
    "moment": "2.14.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/compiler-cli": "~0.6.3",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/platform-server": "~2.1.0",
    "@types/core-js": "0.9.32",
    "@types/d3-array": "0.0.3",
    "@types/d3-brush": "0.0.3",
    "@types/d3-color": "0.0.3",
    "@types/d3-format": "0.0.3",
    "@types/d3-interpolate": "0.0.4",
    "@types/d3-scale": "0.0.3",
    "@types/d3-selection": "0.0.3",
    "@types/d3-shape": "0.0.4",
    "@types/jasmine": "2.2.33",
    "angular2-template-loader": "0.5.0",
    "autoprefixer": "6.4.0",
    "awesome-typescript-loader": "2.2.4",
    "clean-webpack-plugin": "0.1.10",
    "codelyzer": "0.0.28",
    "core-js": "2.4.1",
    "css-loader": "0.24.0",
    "d3": "4.2.2",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "2.22.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.4.1",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "moment": "2.14.1",
    "node-sass": "3.8.0",
    "npm-run-all": "^3.1.0",
    "postcss": "^5.1.1",
    "postcss-loader": "0.11.0",
    "rollup": "^0.34.13",
    "rollup-plugin-sourcemaps": "^0.3.7",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "4.0.0",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.1",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.2",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.3",
    "webpack-notifier": "^1.3.0",
    "zone.js": "0.6.21",
    "uglify-js": "2.7.3"
  }
}
