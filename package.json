{
  "name": "ng2d3",
  "version": "1.5.1",
  "description": "Angular2 + D3 Charting Library",
  "main": "release/ng2d3.js",
  "typings": "release/ng2d3.d.ts",
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "start": "webpack-dev-server --progress --watch --inline",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "build": "webpack --progress --profile",
    "build:release": "NODE_ENV=production npm run build",
    "release": "npm-run-all -s build:release release:css release:ts release:replace release:minify",
    "release:ts": "ngc --outDir release",
    "release:replace": "node tasks/replace.js",
    "release:css": "node-sass -o release/ src/ng2d3.scss",
    "release:minify": "uglifyjs release/ng2d3.js --source-map release/ng2d3.min.js.map --source-map-url release/ng2d3.js.map --compress --mangle --screw-ie8 --output release/ng2d3.min.js",
    "deploy:demo": "node ./tasks/deploy-gh-page.js",
    "profile": "NODE_ENV=production webpack --profile --json > dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ng2d3.git"
  },
  "keywords": [
    "angular2",
    "angularjs",
    "charts",
    "charting",
    "angular",
    "d3",
    "viz",
    "graph",
    "svg"
  ],
  "authors": [
    "Austin McDaniel",
    "Marjan Georgiev",
    "Olivier Combe"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/ng2d3/issues"
  },
  "homepage": "https://github.com/swimlane/ng2d3#readme",
  "peerDependencies": {
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",
    "core-js": "2.4.1",
    "d3": "4.2.2",
    "moment": "2.14.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/compiler-cli": "~2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/forms": "~2.1.2",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",
    "@angular/platform-server": "~2.1.2",
    "@types/core-js": "0.9.32",
    "@types/d3-array": "0.0.3",
    "@types/d3-brush": "0.0.3",
    "@types/d3-color": "0.0.3",
    "replace": "^0.3.0",
    "@types/d3-format": "0.0.3",
    "@types/d3-interpolate": "0.0.4",
    "@types/d3-scale": "0.0.3",
    "@types/d3-selection": "0.0.3",
    "@types/d3-shape": "0.0.4",
    "@types/jasmine": "2.2.33",
    "angular2-template-loader": "0.5.0",
    "autoprefixer": "6.4.0",
    "awesome-typescript-loader": "2.2.4",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "0.1.10",
    "copy-webpack-plugin": "^4.0.0",
    "codelyzer": "0.0.28",
    "core-js": "2.4.1",
    "css-loader": "0.24.0",
    "d3": "4.2.2",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "2.22.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-coverage": "^0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "moment": "2.14.1",
    "node-sass": "3.8.0",
    "npm-run-all": "^3.1.0",
    "postcss": "^5.1.1",
    "postcss-loader": "0.11.0",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "4.0.0",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.1",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.2",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.6",
    "webpack-merge": "^0.15.0",
    "webpack-notifier": "^1.3.0",
    "zone.js": "0.6.26",
    "uglify-js": "2.7.3"
  }
}
