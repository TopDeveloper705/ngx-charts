{
    "gitbook": "3.x.x",
    "title": "ngx-charts",
    "root": "/docs",
    "plugins": [
        "highlight",
        "github",
        "theme-api",
        "include-codeblock",
        "versions",
        "anchors",
        "githubcontributors",
        "collapsible-chapters",
        "youtube"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/swimlane/ngx-charts/"
        },
        "theme-api": {
            "theme": "dark"
        },
        "versions": {
            "type": "tags"
        },
        "githubcontributors": {
          "githubOwner": "swimlane",
          "githubRepository": "ngx-charts"
        }
    }
}
